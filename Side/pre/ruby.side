AEIOU = [?a, ?e, ?i, ?o, ?u]
BCXYZ = [*?a..?z] - AEIOU
def filename
 AEIOU.sample + BCXYZ.sample +  AEIOU.sample + BCXYZ.sample +  AEIOU.sample
end

def avail
 loop do
  x = filename + ".rb"
  return x if !FileTest.exists?(x)
 end 
end

open(name = avail, "w"){|f| f.write "puts 'Hello world'"}
system 'cmd /c start "" notepad ' + name
 
